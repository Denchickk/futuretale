// Freeze Player

obj_player.cantmove = 1;

// Get rid of C-Menu

with (obj_cmenu){
instance_destroy();
}

scr_input();

length = string_length(message[currentmessage]);

messagedrawn = string_copy(message[currentmessage],0,character);
if (character&lt;length){
character += 1;
if (string_char_at(message[currentmessage],character) != " "){
audio_play_sound(sound,9,false);
